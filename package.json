{
  "name": "arwes",
  "version": "1.0.0",
  "description": "Futuristic Sci-Fi and Cyberpunk Graphical User Interface Framework for Web Apps",
  "keywords": [
    "gui",
    "ui",
    "interface",
    "interfaces",
    "futuristic",
    "sci-fi",
    "cyberpunk",
    "high-tech",
    "browser",
    "prhone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romelperez/arwes.git"
  },
  "bugs": {
    "url": "https://github.com/romelperez/arwes/issues"
  },
  "author": "Romel PÃ©rez (romelperez.com)",
  "license": "MIT",
  "homepage": "https://github.com/yvanovitch/arwes",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test-lint && TRAVIS=true npm run test-dev",
    "test-lint": "eslint --cache --quiet ./src",
    "test-dev": "karma start",
    "compile": "npm run compile-clean && npm run compile-run",
    "compile-clean": "rimraf ./lib/*",
    "compile-run": "webpack --config webpack/lib.config.js",
    "watch": "webpack --config webpack/play.config.js",
    "play": "webpack-dev-server  --config webpack/play.config.js",
    "script-api": "node ./scripts/api",
    "site-dev": "npm run script-api && next -p 7000",
    "site-build": "npm run script-api && next build",
    "site-start": "next start -p 17000",
    "site-deploy": "pm2 startOrRestart deploy.json --env production"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "d3-path": "^1.0.9",
    "d3-shape": "^1.3.7",
    "detect-node": "^2.0.4",
    "extend": "^3.0.2",
    "hoist-non-react-statics": "^3.3.2",
    "howler": "^2.1.3",
    "polished": "^3.5.2",
    "prismjs": "^1.20.0",
    "prop-types": "*",
    "react": "^16.13.1",
    "react-jss": "^10.1.1",
    "react-transition-group": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "karma": "^5.0.2",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "marksy": "^8.0.0",
    "mocha": "^7.1.1",
    "navigo": "^7.1.2",
    "next": "^9.3.5",
    "raw-loader": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-docgen": "^5.3.0",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-group": "^3.0.2",
    "react-live": "^2.2.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  }
}
